<swiper indicator-dots="true" autoplay="true" interval="3000" duration="2000">
  <block wx:for="{{swiperUrls}}" wx:key="*this">
    <swiper-item>
      <image src="{{item.url}}" class="img"></image>
    </swiper-item>
  </block>
</swiper>
<view class="playListContent">
  <block wx:for="{{playListData}}" wx:key="_id">
    <my-playList playList="{{item}}"></my-playList>
  </block>
</view>
